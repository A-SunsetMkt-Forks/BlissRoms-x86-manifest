           sync-c="true"
           sync-j="4"  />

  <project path="manifest" name="platform_manifest" remote="BlissRoms" />
  <project path="frameworks/base" name="platform_frameworks_base" remote="BlissRoms" />
  <project path="packages/apps/Blissify" name="platform_packages_apps_Blissify" groups="pdk-fs" remote="BlissRoms" />
  <project path="packages/apps/Settings" name="platform_packages_apps_Settings" groups="pdk-fs" remote="BlissRoms" />
</manifest>
